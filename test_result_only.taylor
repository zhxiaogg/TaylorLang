// Simple Result pattern test only
type Result<T, E> = Ok(T) | Error(E)

val result = Ok(42)
val processedResult = match result {
    case Ok(value) => "Success: " + value
    case Error(err) => "Error: " + err
}
assert(processedResult == "Success: 42")

val errorResult = Error("not found")
val errorMessage = match errorResult {
    case Ok(data) => "Got: " + data
    case Error(msg) => "Error: " + msg
}
assert(errorMessage == "Error: not found")

println("Simple Result pattern tests completed!")