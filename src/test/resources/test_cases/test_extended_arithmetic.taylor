// Extended arithmetic operations
assert(15 - 12 == 3)
assert(2.5 * 3.0 == 7.5)
assert(10 / 3.0 == 3.3333333333333335)

// Operator precedence
assert(2 + 3 * 4 == 14)        // Should be: 14 (not 20)
assert((2 + 3) * 4 == 20)

// Comparison chains
assert((5 < 10 && 10 < 20) == true)
assert((1 == 1 && 2 != 3) == true)
assert(!(false || false) == true)

// Mixed type comparisons
assert((5 == 5.0) == true)
assert((3.14 > 3) == true)

// Additional arithmetic operations
assert(20 / 5 == 4)
assert(8 - 7 == 1)
assert(8.0 / 2.0 == 4.0)

// More comparison operations
assert((10 >= 10) == true)
assert((5 <= 4) == false)
assert((3 != 4) == true)
assert((7 == 7) == true)

// Complex expressions with precedence
assert(10 - 5 + 2 == 7)
assert(3 * 4 + 2 * 5 == 22)
assert(20 / 4 - 1 == 4)