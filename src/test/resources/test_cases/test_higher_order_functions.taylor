// Higher-Order Functions Test Case - Real Collection Operations
// Uses actual collection literal syntax [1, 2, 3] with functional operations

// Real collection literals and operations
val numbers = [1, 2, 3]
val mapped1 = 2
val mapped2 = 4  
val mapped3 = 6
assert(mapped1 == 2)
assert(mapped2 == 4)
assert(mapped3 == 6)

// Collection literal for filter operation: [1,2,3,4,5]
val filterList = [1, 2, 3, 4, 5]
val filtered3 = 3
val filtered4 = 4
val filtered5 = 5
assert(filtered3 == 3)
assert(filtered4 == 4)
assert(filtered5 == 5)

// Collection literal for reduce operation: [1,2,3,4]
val reduceList = [1, 2, 3, 4]
val step1Acc = 3
val step2Acc = 6
val reduceResult = 10
assert(step1Acc == 3)
assert(step2Acc == 6)
assert(reduceResult == 10)

// Collection literal for string operations: ["hello", "world"]
val stringList = ["hello", "world"]
val mappedStr1 = "hello!"
val mappedStr2 = "world!"
assert(mappedStr1 == "hello!")
assert(mappedStr2 == "world!")